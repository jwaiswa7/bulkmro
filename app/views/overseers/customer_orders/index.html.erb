<%= provide :container_fluid, true %>
<%# provide :can_be_directly_created, true %>
<% provide :can_be_ajaxed, true %>
<% provide :wrap_whitespace, true %>

<% content_for :header_row do %>
  <th class="no-sort all" data-width="70px"></th>
  <th class="no-sort desktop text-right">Online Order #</th>
  <th class="no-sort desktop">Status</th>
  <th class="no-sort desktop">Payment Method</th>
  <th class="no-sort desktop">Account</th>
  <th class="no-sort all">Company</th>
  <th class="no-sort all">Customer Name</th>
  <th class="no-sort all text-right">Line Items</th>
  <th class="sort all text-right">Total (Excl. Tax)</th>
  <th class="sort all text-right" data-name="grand_total">Total (Incl. Tax)</th>
  <th class="no-sort all">Inside Sales</th>
  <th class="no-sort all">Outside Sales</th>
  <th class="no-sort all">Manager</th>
  <th class="sort desktop text-right" data-name="created_at">Created</th>
<% end %>

<div class="col-lg-12">
  <%= render layout: 'shared/layouts/datatable' do %>
  <% end %>
</div>