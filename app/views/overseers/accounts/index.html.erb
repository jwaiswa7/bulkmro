<a href="<%= new_overseers_account_path %>">
  <button type="button" class="btn btn-primary m-t-10">+ Create New Account</button>
</a>

<hr />

<div class="table-responsive">
  <table id="datatable-buttons" class="table table-striped table-bordered" cellspacing="0" width="100%">
    <thead>
      <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Actions</th>
      </tr>
    </thead>
    <tbody>
      <% @accounts.each do |account| %>
        <tr>
          <td><%= account.id %></td>
          <td><%= account.name %></td>
          <td>
            <a href="<%= edit_overseers_account_path(account) %>">Edit</a>
            <a href="<%= overseers_account_path(account) %>">Show</a>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>