<div class="col-lg-12 pk">
  <%= render 'shared/snippets/page_header', e: @account %>
  <%= simple_form_for([:overseers, @account], :html => {:class => 'needs-validation', :'novalidate' => '', :'data-parsley-validate' => ''}) do |f| %>
    <%= render 'shared/snippets/form_errors', f: f %>
    <div class="form-group ">
      <%= f.input :name, label: 'Alias', :placeholder => 'e.g. Reliance Industries', :required => 'true', :class => 'form-control bmro-form-input' %>
    </div>

    <div class="form-group">
      <%= f.input :account_type, :wrapper => :custom_multi_select, :class => 'form-control bmro-form-input', as: :select, collection: enum_to_collection(Account.account_types) %>
    </div>
    <div class="bmro-button">
       <%= action_button(action_name, @account) %>
    </div>
  <% end %>
</div>




