<div class="col-lg-12 pk">
  <%= render 'shared/snippets/page_header', e: @account %>
  <%= simple_form_for([:overseers, @account], :html => {:class => 'needs-validation', :'novalidate' => '', :'data-parsley-validate' => ''}) do |f| %>
    <%= render 'shared/snippets/form_errors', f: f %>
    <div class="form-group bmro-input-label bmro-form-input">
      <%= f.input :name, label: 'Alias', :placeholder => 'e.g. Reliance Industries', :required => 'true',input_html: { class: 'bmro-form-input' }, :class => 'form-control bmro-form-input' %>
    </div>

    <div class="form-group bmro-input-label">
      <%= f.input :account_type, :wrapper => :custom_multi_select,input_html: { class: 'bmro-form-input bmro-dropdown-icon' }, :class => 'form-control bmro-form-input bmro-dropdown-icon', as: :select, collection: enum_to_collection(Account.account_types) %>
    </div>
    <div class="bmro-button-parent">
       <%= action_button(action_name, @account) %>
    </div>
  <% end %>
</div>




