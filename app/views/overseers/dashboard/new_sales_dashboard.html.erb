    $(".bmro-close-right-slide").click(function () {
        $('.bmro-slide-on-inquries').removeClass('bmro-inquries-po');
        // $(this).removeClass('bmro-close-right-slide-fix');
    })
    $(".bmro-right-side-open").click(function () {
        $('.bmro-slide-order-no').addClass('bmro-slide-order-show');
        $('.bmro-slide-on-inquries').removeClass('bmro-inquries-po');
        // $('.bmro-close-order-slide').addClass('bmro-close-right-slide-fix',15000);
    })
