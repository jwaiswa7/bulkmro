          <div class="brmo-today-list ">
            <h5 class="bmro-big-heading bmro-hash-mt">#<%= notification.id %> <span class="bmro-today-button">Notification</span></h5>
            <span class="bmro-today-span"><%= notification.message %></span>
            <span class="bmro-today-posted">Posted on: <%= notification.created_at.strftime("%d/%m/%Y %I:%M %P") %></span>
          </div>
        <% end %>
      <% end %>
    </div>

    <div class="bmro-width100" id="comment-rows">
      <% @dashboard.comments.each do |date, comments| %>
        <div class="brmo-today-list bmro-today-mt-mb">
          <div class="bmro-width100 brmo-today-line-parent">
            <div class="bmro-today-parent">
              <% if date.today? %>
                <span class="bmro-big-heading bmro-fl">Today</span>
              <% else %>
                <span class="bmro-big-heading bmro-fl"><%= date.strftime("%d/%m/%Y") %></span>
              <% end %>
            </div>
            <span class="bmro-today-line"></span>
          </div>
        </div>
        <% comments.each do |comment| %>
          <div class="brmo-today-list ">
            <h5 class="bmro-big-heading bmro-hash-mt">#<%= comment.id %> <span class="bmro-today-button">Comment</span></h5>
            <span class="bmro-today-span"><%= comment.message %></span>
            <h5 class="bmro-big-heading bmro-hash-mt"><%= comment.created_by.to_s %> <span class="bmro-today-button"><%= comment.created_by.role %></span></h5>
            <span class="bmro-today-posted">Posted on: <%= comment.created_at.strftime("%d/%m/%Y %I:%M %P") %></span>
          </div>
        <% end %>
      <% end %>
    </div>
  </div>
</div>



<script>
    $( document ).ready(function() {
      $("#notifications").trigger('click');
    });

    $(".bmro-menu-click").click(function () {
        $('.bmro-dash-main-menu').toggleClass('bmro-dash-main-menu-show');
        $(this).toggleClass('bmro-menu-roted');
    });

    $('.bmro-main-drop-col p').hover(function(){
        $(this).children('.bmro-dash-hover').css("display","block");
    }, function() {
        $(this).children('.bmro-dash-hover').css("display","none");
    });

    $(".bmro-dashbord-click").click(function () {
        $('.bmro-dash-main-menu').removeClass('bmro-dash-main-menu-show');
        $('.bmro-menu-click').removeClass('bmro-menu-roted');
    });

    $(".bmro-status-label").click(function (e) {
        let inquiry_number = $(this).data('id');
        e.preventDefault();
        $.ajax({
            url: "dashboard/get_status_records",
            type: "GET",
            data: {
                inquiry_number: inquiry_number
            },
            success: function (data) {
              $('#status-record-div').empty();
              $('#status-record-div').append(data);
            },
        });
        $('.bmro-slide-order-no').removeClass('bmro-slide-order-show');
        $('.bmro-slide-on-inquries').addClass('bmro-inquries-po');
        // $('.bmro-close-right-slide').addClass('bmro-close-right-slide-fix',15000);
    });

    $(".bmro-close-right-slide").click(function () {
        $('.bmro-slide-on-inquries').removeClass('bmro-inquries-po');
        // $(this).removeClass('bmro-close-right-slide-fix');
    });

    $(".bmro-right-side-open").click(function () {
        $('.bmro-slide-order-no').addClass('bmro-slide-order-show');
        $('.bmro-slide-on-inquries').removeClass('bmro-inquries-po');
        // $('.bmro-close-order-slide').addClass('bmro-close-right-slide-fix',15000);
    });
    $(".bmro-close-order-slide").click(function () {
        // $(this).removeClass('bmro-close-right-slide-fix');
        $('.bmro-slide-order-no').removeClass('bmro-slide-order-show');
    });

    $(".bmro-same-box").click(function () {
        $('.bmro-same-box').removeClass('bmro-same-box-active');
        $(this).addClass('bmro-same-box-active',1000);
        $('.bmro-sales-main').removeClass('bmro-sales-show-hide');
        $('.bmro-inquries-main').addClass('bmro-inquiry-show-hide');
    });

    $(".bmro-sales-back").click(function () {
        $('.bmro-same-box').removeClass('bmro-same-box-active');
        $('.bmro-sales-main').addClass('bmro-sales-show-hide');
        $('.bmro-inquries-main').removeClass('bmro-inquiry-show-hide');
    });

    $(".bmro-order-show").click(function () {
        $('.bmro-order-action').removeClass('bmro-order-hide');
        $('.bmro-all-task-action').addClass('bmro-order-hide');
        $('.bmro-inquries-main').addClass('bmro-bg-color');
        $('.bmro-Inquries-task').removeClass('bmro-active-white');
        $(this).parent().parent().addClass('bmro-active-white',1000);
        $('.bmro-show-star').addClass('bmro-inquiry-show-hide');
        $('.bmro-reset-button').removeClass('bmro-inquiry-show-hide');
    });

    $(".bmro-reset-button").click(function () {
        $('.bmro-order-action').addClass('bmro-order-hide');
        $('.bmro-all-task-action').removeClass('bmro-order-hide');
        $('.bmro-inquries-main').removeClass('bmro-bg-color');
        $('.bmro-Inquries-task').removeClass('bmro-active-white');
        $('.bmro-show-star').removeClass('bmro-inquiry-show-hide');
        $('.bmro-reset-button').addClass('bmro-inquiry-show-hide');
    });

    $("#notifications").click(function () {
        $('#comment-rows').hide();
        $('#notification-rows').show();
    });

    $('#comments').click(function () {
        $('#notification-rows').hide();
        $('#comment-rows').show();
    })
</script>