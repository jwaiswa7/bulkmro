<% provide :container_fluid, true %>
<% provide :can_be_ajaxed, true %>
<% provide :wrap_whitespace, true %>
<% provide :show_date_filter, true %>
<% content_for :heading do %>
  Smart Queue
<% end %>

<% content_for :header_row do %>
  <th class="no-sort all" width="98px"> Quick Actions</th>
  <th class="no-sort desktop text-left">Priority</th>
  <th class="sort all text-right" data-name="inquiry_number">#</th>
  <th class="no-sort desktop text-left" data-name="status">Status</th>
  <th class="no-sort desktop text-left" data-name="account_id">Account</th>
  <th class="no-sort desktop text-left" data-name="company_id">Company</th>
  <th class="no-sort desktop text-left" data-name="contact_id">Contact</th>
  <th class="no-sort desktop text-left" data-name="inside_sales_executive">Inside Sales</th>
  <th class="no-sort desktop text-left" data-name="outside_sales_executive">Outside Sales</th>
  <th class="sort desktop text-right" data-name="calculated_total">Total</th>
  <th class="sort desktop text-left" data-name="created_at">Created</th>
<% end %>

<% content_for :filter_row do %>
  <td data-filter="false"></td>
  <td data-filter="false"></td>
  <td data-filter="false"></td>
  <td data-filter="dropdown"></td>
  <td data-filter="ajax"></td>
  <td data-filter="ajax"></td>
  <td data-filter="ajax"></td>
  <td data-filter="dropdown"></td>
  <td data-filter="dropdown"></td>
  <td data-filter="false"></td>
  <td data-filter="daterange"></td>
<% end %>

<div class="col-lg-12">
  <%= render layout: 'shared/layouts/datatable' do %>
  <% end %>
</div>