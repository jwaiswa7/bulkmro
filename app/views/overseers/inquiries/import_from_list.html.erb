<%= render layout: 'shared/layouts/form' do %>
  <%= simple_form_for(:import_from_list, url: handle_list_overseers_inquiry_path(@inquiry), :html => { :class => 'card needs-validation mt-3', :'novalidate' => '', :'data-parsley-validate' => ''}) do |f| %>
    <div class="card-body">
      <div class="form-group">
        <%= f.input :list_text, as: :text, rows: 10 %>
      </div>

      <div class="form-group mb-0">
        <%= f.submit(class: 'btn btn-success btn-block') %>
      </div>
    </div>
  <% end %>
<% end %>