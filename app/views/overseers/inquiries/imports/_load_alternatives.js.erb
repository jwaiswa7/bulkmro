'<%= simple_form_for(@excel_import, url: create_failed_skus_overseers_inquiry_import_path, :html => {:class => 'needs-validation mt-3', :'novalidate' => '', :'data-parsley-validate' => ''}) do |f| %>'
  '<%= f.simple_fields_for :rows, f.object.rows.reverse do |row| %>'
    var name = 'name="inquiry_import[rows_attributes][<%= item_index %>][approved_alternative_id]"';
    var html = "<%= escape_javascript(render( :partial => 'approved_alternatives', :locals => { row_object: row_object, row: row, page: page } )) %>";

    html = html.replace(/name="[a-z\d_\[\]]*"/g, name)

    $('.' + '<%= row_object.id %>' + '.card-footer').attr('data-page', <%= page %>);
    $('.' + '<%= row_object.id %>' + '.approved-alternatives').empty();
    $('.' + '<%= row_object.id %>' + '.approved-alternatives').append(html);
  '<% end %>'
'<% end %>'.html_safe
