<p id="notice"><%= notice %></p>

<h1>Material Dispatches</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @material_dispatches.each do |material_dispatch| %>
      <tr>
        <td><%= link_to 'Show', material_dispatch %></td>
        <td><%= link_to 'Edit', edit_material_dispatch_path(material_dispatch) %></td>
        <td><%= link_to 'Destroy', material_dispatch, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Material Dispatch', new_material_dispatch_path %>
