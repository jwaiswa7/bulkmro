  <th class="no-sort" data-name="order_number">Order #</th>
  <th class="no-sort" data-name="order_number">Inquiry #</th>
  <th class="no-sort" data-name="status">Status</th>
  <th class="no-sort" data-name="remote-status">SAP Status</th>
  <th class="no-sort" data-name="">Account</th>
  <th class="no-sort" data-name="">Company</th>
  <th class="no-sort" data-name="">Inside Sales</th>
  <th class="no-sort" data-name="">Outside Sales</th>
