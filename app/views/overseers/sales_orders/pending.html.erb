<% provide :container_fluid, true %>
<% provide :can_be_ajaxed, true %>
<% provide :wrap_whitespace, true %>
<% content_for :heading do %>
  Pending Sales Orders
<% end %>

<% content_for :header_row do %>
  <th class="no-sort"></th>
  <th class="no-sort" data-name="order_number">Inquiry #</th>
  <th class="no-sort" data-name="">Inside Sales</th>
  <th class="no-sort" data-name="created_at">Requested Date</th>
  <th class="no-sort" data-name="">Company</th>
  <th class="no-sort" data-name="">Outside Sales</th>
  <th class="no-sort" data-name="">Customer PO #</th>
  <th class="no-sort" data-name="" data-toggle="tooltip" title="Grand Total (Excluding Tax)">G. Total (E. Tax)</th>
  <th class="no-sort" data-name="" data-toggle="tooltip" title="Grand Total (Including Tax)">G. Total (I. Tax)</th>
  <th class="no-sort" data-name="" data-toggle="tooltip" title="Margin (Excluding Tax)">Margin (E. Tax)</th>
<% end %>

<div class="col-lg-12">
  <%= render layout: 'shared/layouts/datatable' do %>
  <% end %>
</div>