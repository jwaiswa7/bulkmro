

var tbodyRef = document.getElementById('stock-table').getElementsByTagName('tbody')[0];
var i = 0

<% @product.stocks.order(instock: :desc).each do |stock| %>
  var row = tbodyRef.insertRow(i);
  var cell1 = row.insertCell(0);
  var cell2 = row.insertCell(1);
  var cell3 = row.insertCell(2);
  var cell4 = row.insertCell(3);
  var cell5 = row.insertCell(4);
  cell1.innerHTML = "<%= stock.warehouse.name %>";
  cell2.innerHTML = "<%= stock.instock %>";
  cell3.innerHTML = "<%= stock.committed %>";
  cell4.innerHTML = "<%= stock.ordered %>";
  cell5.innerHTML = "<%= (stock.instock - stock.committed) %>";
  i = i + 1
<% end %>

$('#inStock').modal('show');