<%= provide :container_fluid, true %>
<% provide :can_be_directly_created, true %>
<% provide :can_be_ajaxed, true %>
<% provide :wrap_whitespace, true %>

<% content_for :header_row do %>
  <th class="no-sort all" data-width="70px"></th>
  <th class="no-sort all">Model Name</th>
  <th class="no-sort all">Action name</th>
  <th class="no-sort desktop text-right">Created At</th>
  <th class="no-sort desktop text-right">Updated At</th>
  <th class="no-sort desktop text-right">Created By</th>
  <th class="no-sort desktop text-right">Updated By</th>
<% end %>

<div class="col-lg-12">
  <%= render layout: 'shared/layouts/datatable' do %>
  <% end %>
</div>
