
<div class="table-responsive">
  <table id="datatable-buttons" class="table table-striped table-bordered" cellspacing="0" width="100%">
    <thead>
    <tr>
      <th>Id</th>
      <th>Name</th>
      <th>Actions</th>
    </tr>
    </thead>
    <tbody>
    <% @companies.each do |company| %>
      <tr>
        <td><%= company.id %></td>
        <td><%= company.name %></td>
        <td>
          <a href="<%= edit_overseers_account_company_path(company.account, company) %>">Edit</a>
          <a href="<%= overseers_company_path(company) %>">Show</a>
        </td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>