<div class="tab-pane fade active show" id="banks" role="tabpanel">
  <% content_for :ajax_url, flush: true do %><%= overseers_company_company_banks_path(company_id: company.id) %>
  <% end %>
  <% content_for :header_row, flush: true do %>
    <th class="no-sort all" width="98px">Quick Actions</th>
    <th class="all" data-name="name">Name</th>
    <th class="all" data-name="code">Code</th>
    <th class="all" data-name="branch">Branch</th>
    <th class="all" data-name="ifsc_code">IFSC Code</th>
    <th class="all" data-name="account_name">Account Name</th>
    <th class="all" data-name="account_number">Account Number</th>
    <th class="desktop" data-name="created_at">SAP Status</th>
    <th class="desktop text-right" data-name="created_at">Created</th>
  <% end %>
  <% content_for :heading_buttons_internal_datatable, flush: true do %>
    <% if is_authorized(:contact, 'new') %>
      <a href="<%= new_overseers_company_company_bank_path(company_id: company.to_param) %>" class="btn btn-success btn-rounded align-self-center btn-block-sm-down mb-2 mb-md-0 bmro-button"><i class="fal fa-plus-circle mr-1"></i>New
        Bank</a>
    <% end %>
  <% end %>
  <%= render 'shared/layouts/internal_datatable', entity: company, children: 'banks' %>
</div>
