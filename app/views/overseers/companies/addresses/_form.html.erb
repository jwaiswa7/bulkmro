company under <%= @address.company %>

<%= simple_form_for([:overseers, @company, @address]) do |f| %>
  <%= f.hidden_field :company_id %>
  <%= f.input :created_by, disabled: true %>
  <%= f.input :created_at, disabled: true %>
  <%= f.input :updated_by, disabled: true %>
  <%= f.input :updated_at, disabled: true %>

  <%= f.input :name %>

  <%= f.country_select :country_code, priority_countries: ["IN", "US"] %>

  <%= f.input :pincode %>

  <%= f.association :state %>
  <%= f.input :state_name %>

  <%= f.input :city_name %>

  <%= f.input :street1, as: :text %>
  <%= f.input :street2, as: :text  %>
  
  <%= f.input :telephone  %>
  <%= f.input :mobile  %>  

  <%= f.input :tan  %>
  <%= f.input :gst  %>
  <%= f.file_field :gst_proof, :class => '', required: true, accept: 'image/png, application/pdf, image/jpeg' %>
  <%= f.input :cst  %>
  <%= f.file_field :cst_proof, :class => '', required: true, accept: 'image/png, application/pdf, image/jpeg' %>
  <%= f.input :vat  %>
  <%= f.file_field :vat_proof, :class => '', required: true, accept: 'image/png, application/pdf, image/jpeg' %>
  <%= f.input :excise  %>
  <%= f.file_field :excise_proof, :class => '', required: true, accept: 'image/png, application/pdf, image/jpeg' %>

  <%= f.input :gst_type, as: :select, collection: enum_to_collection(Address.gst_types) %>


  <%= f.submit %>
<% end %>