<%= provide :container_fluid, true %>
<% provide :can_be_directly_created, true %>
<% provide :can_be_ajaxed, true %>
<% provide :show_date_filter, true %>
<% content_for :page_title do %>
  IFSC Codes
<% end %>

<% content_for :heading do %>
  IFSC Codes
<% end %>

<% content_for :header_row do %>
  <th class="no-sort" width="98px">Quick Actions</th>
  <th class="no-sort text-left" data-name="name">IFSC Code</th>
  <th class="no-sort desktop text-left ">Branch</th>
  <th class="no-sort desktop text-left">Address</th>
  <th class="no-sort desktop text-left">City</th>
  <th class="no-sort desktop text-left">District</th>
  <th class="no-sort desktop text-left">State</th>
  <th class="sort desktop text-left"  data-name="created_at">Created At</th>
<% end %>

<% content_for :filter_row do %>
  <td data-filter="false"></td>
  <td data-filter="false"></td>
  <td data-filter="false"></td>
  <td data-filter="false"></td>
  <td data-filter="false"></td>
  <td data-filter="false"></td>
  <td data-filter="false"></td>
  <td data-filter="daterange"></td>
<% end %>

<div class="col-lg-12">
  <%= render layout: 'shared/layouts/datatable' do %>
  <% end %>
</div>