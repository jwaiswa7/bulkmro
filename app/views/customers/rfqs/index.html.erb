<% provide :container_fluid, true %>
<% provide :can_be_ajaxed, true %>
<% provide :wrap_whitespace, true %>
<% provide :use_url_as_page_title, true %>
<% provide :can_be_directly_exported_partial, true %>
<% provide :show_date_filter, true %>
<% provide :can_be_directly_created, policy(:rfq).new? %>
<% content_for :heading do %>
  RFQs
<% end %>

<div class="col-lg-12">
  <% content_for :header_row do %>
    <th class="no-sort all"></th>
    <th class="no-sort all" data-name="inquiry_number">Inquiry #</th>
    <th class="sort all " data-name="created_at">Inquiry Date</th>
    <th class="no-sort desktop">Subject</th>
    <th class="no-sort desktop">Sales Person</th>
  <% end %>

  <% content_for :filter_row do %>
    <td data-filter="false"></td>
    <td data-filter="false"></td>
    <td data-filter="false"></td>
    <td data-filter="false"></td>
    <td data-filter="false"></td>
  <% end %>

  <%= render layout: 'shared/layouts/datatable' do %>
  <% end %>
</div>
