                <td align="center" valign="top">
                  <span></span></td>
                <% if record.sales_order.sales_quote.tax_summary.strip == '0' ||
                    (record.sales_order.sales_quote.tax_summary.to_s.include? "IGST") %>
                  <td align="center" valign="top">
                    <span></span></td>
                  <td align="right" valign="top">
                    <b><span><%= number_to_currency(record.metadata['base_tax_amount'].to_f / record.metadata['base_to_order_rate'].to_f, precision: 2, unit: '') %></span></b>
                  </td>
                <% else %>
                  <td align="center" valign="top">
                    <span></span></td>
                  <td align="right" valign="top">
                    <b><span><%= number_to_currency(((record.metadata['base_tax_amount'].to_f / record.metadata['base_to_order_rate'].to_f) / 2), precision: 2, unit: '') %></span></b>
                  </td>
                  <td align="center" valign="top">
                    <span></span></td>
                  <td align="right" valign="top">
                    <b><span><%= number_to_currency(((record.metadata['base_tax_amount'].to_f / record.metadata['base_to_order_rate'].to_f) / 2), precision: 2, unit: '') %></span></b>
                  </td>
                <% end %>
                <td align="right" valign="top">
                  <b><span><%= number_to_currency((record.metadata['base_grand_total'].to_f / record.metadata['base_to_order_rate'].to_f), precision: 2, unit: '') %></span></b>
                </td>
              </tr>

              </tbody>
            </table>
          </div>
          <br clear="all">
          <div class="bottom-info" style="position: relative">
            <% if params[:stamp] %>
              <div id="stamp" style="width:94%;position:absolute;top:0px; padding-top: 1px;padding-bottom: 1px;">
